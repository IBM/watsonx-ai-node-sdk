  "dependencies": {
    "@ibm-cloud/watsonx-ai": "file:.yalc/@ibm-cloud/watsonx-ai",
    "@langchain/community": "^0.3.57",
    "@langchain/core": "^0.3.19",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@langchain/langgraph": "^0.2.20",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "path": "^0.12.7",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2"
  }
}
