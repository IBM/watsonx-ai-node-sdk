{
  "jest.jestCommandLine": "npm run test --",
  "jest.virtualFolders": [
    {
      "name": "unit-tests",
      "rootPath": "test/unit",
      "jestCommandLine": "jest -c jest.unit.config.js"
    },
    {
      "name": "integration-tests",
      "rootPath": "test/integration",
      "jestCommandLine": "jest -c jest.int.config.js"
    },
    { "name": "example-tests", "rootPath": "examples/test" },
    {
      "name": "langchain-regression-tests",
      "rootPath": "test/langchain/test"
    }
  ],
  "jest.runMode": "on-demand"
}
